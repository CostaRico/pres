{{mobile-menu opened=isMobileMenuOpened}}

{{header-c isMobileMenuOpened=isMobileMenuOpened }}

<div class="presentation {{if isMobileMenuOpened 'blur'}}">


  {{#if editMode}}
    {{editable/first-page model=model}}
  {{else}}
    {{first-page model=model}}
  {{/if}}


    <div class="presentation__slide second-page">
        {{buildings-view model=model}}
    </div>

    <div class="presentation__slide">
        {{buildings-props model=model}}
    </div>

    <div class="presentation__slide">
      {{bc-gallery model=model}}
    </div>

    <div class="presentation__slide gray">
        {{buildings-features model=model}}
    </div>


    <div class="presentation__slide">
        {{offices-table model=model}}
    </div>

    <div class="presentation__slide gray">
        {{contacts-c model=model}}
    </div>


</div>